{
    "extends": ["airbnb", "prettier", "plugin:node/recommended"],
    "plugins": ["prettier"],
    "rules": {
      "no-unused-vars": "off",
      "no-console": "off",
      "func-names": "off",
      "no-process-exit": "off",
      "object-shorthand": "off",
      "class-methods-use-this": "off",
      "consistent-return": "off",
      "camelcase": "off",
      "no-plusplus": "off",
      "no-use-before-define": "off",
      "no-undef": "off",
      "max-classes-per-file": "off",
      "prefer-template": "off",
      "no-param-reassign": "off",
      "prefer-const": "error"
    }
  }